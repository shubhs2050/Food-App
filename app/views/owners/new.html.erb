<h1>Add Owners</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for Owner.new do |f| %>
    <%= render 'shared/owner_messages' %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>

      <%= f.submit "Add owner", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
