<% provide(:title, "Log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag "/ologin", :osessions => :login_attempt do %>
     <p>OwnerName : </br> <%= text_field_tag(:name) %></p>
     <p>Email : </br> <%= email_field_tag :email %></p>
     <%= submit_tag("Log In")%>
    <% end %>
  </div>
</div>
